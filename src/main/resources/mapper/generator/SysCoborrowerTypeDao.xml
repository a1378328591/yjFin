<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yjcaifu.common.dao.SysCoborrowerTypeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.yjcaifu.common.entity.SysCoborrowerTypeEntity" id="sysCoborrowerTypeMap">
        <result property="id" column="id"/>
        <result property="type" column="type"/>
    </resultMap>
	
	<!-- 根据userId查询用户信息 -->
	<select id="queryAllList" resultType="com.yjcaifu.common.entity.SysCoborrowerTypeEntity">
			select * from sys_coborrower_type where 1=1
	</select>

</mapper>